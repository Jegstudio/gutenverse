{
  "name": "gutenverse",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development & npm run start --prefix gutenverse-core & npm run start:js & npm run start:css",
    "build": "cross-env NODE_ENV=production & npm run build --prefix gutenverse-core & npm run build:js & npm run build:css",
    "start:js": "webpack --config .config/webpack.config.dev.js --watch",
    "build:js": "webpack --config .config/webpack.config.prod.js",
    "start:css": "gulp watch",
    "build:css": "gulp build",
    "development": "cross-env NODE_ENV=development && npm run development --prefix gutenverse-core && gulp clean && npm run build && npm run pot && gulp release",
    "production": "cross-env NODE_ENV=production && npm run production --prefix gutenverse-core && gulp clean && npm run build && npm run pot && gulp release",
    "format:js": "wp-scripts format-js",
    "fix": "npx eslint --fix ./src || echo \"Formatting done! Please fix the remaining warnings and errors\"",
    "fix-pkg": "npx eslint --fix ./gutenverse-core || echo \"Formatting done! Please fix the remaining warnings and errors\"",
    "create": "node ./scripts/create-block.js",
    "new": "node ./scripts/new-block.js",
    "pot": "wp i18n make-pot ./gutenverse ./gutenverse/languages/gutenverse.pot --domain=gutenverse --exclude=\"lib/framework/\"",
    "jsonpot": "wp i18n make-json ./gutenverse/languages/",
    "compress": "node ./scripts/compress-files.js"
  },
  "repository": {
    "type": "git",
    "url": "team@207.148.22.34:/repository/gutenverse"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@wordpress/scripts": "^12.5.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^6.0.1",
    "filemanager-webpack-plugin": "^8.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-zip": "^5.1.0",
    "postcss": "^8.4.23",
    "sass": "^1.62.1",
    "webpack": "^5.78.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@wordpress/icons": "^9.23.0",
    "animejs": "^3.2.1",
    "choices.js": "^10.2.0",
    "classnames": "^2.3.2",
    "crypto-random-string": "^5.0.0",
    "flatpickr": "^4.6.13",
    "gutenverse-core": "file:gutenverse-core",
    "react-datepicker": "^4.11.0",
    "react-feather": "^2.0.10",
    "react-id-swiper": "^4.0.0",
    "react-player": "^2.12.0",
    "react-select": "^3.1.1",
    "shufflejs": "^6.1.0",
    "swiper": "https://gitpkg.now.sh/jegbagus/swiper/dist?master",
    "umbrellajs": "^3.3.1"
  }
}
