{
  "apiVersion": 2,
  "name": "gutenverse/google-maps",
  "title": "Google Maps",
  "description": "Display a location you want to share to your website visitor.",
  "category": "gutenverse-element",
  "attributes": {
    "elementId": {
      "type": "string"
    },
    "location": {
      "type": "string",
      "default": "New York"
    },
    "zoom": {
      "type": "string",
      "default": "13"
    },
    "height": {
      "type": "object"
    },
    "mapFilter": {
      "type": "object"
    },
    "mapFilterHover": {
      "type": "object"
    },
    "background": {
      "type": "object",
      "default": {}
    },
    "backgroundHover": {
      "type": "object",
      "default": {}
    },
    "border": {
      "type": "object",
      "default": {}
    },
    "borderHover": {
      "type": "object",
      "default": {}
    },
    "boxShadow": {
      "type": "object"
    },
    "boxShadowHover": {
      "type": "object"
    },
    "hideDesktop": {
      "type": "boolean"
    },
    "hideTablet": {
      "type": "boolean"
    },
    "hideMobile": {
      "type": "boolean"
    },
    "animation": {
      "type": "object",
      "default": {}
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    },
    "zIndex": {
      "type": "object"
    },
    "positioningType": {
      "type": "object"
    },
    "positioningWidth": {
      "type": "object"
    },
    "positioningAlign": {
      "type": "object"
    },
    "positioningLocation": {
      "type": "string"
    },
    "positioningLeft": {
      "type": "object"
    },
    "positioningRight": {
      "type": "object"
    },
    "positioningTop": {
      "type": "object"
    },
    "positioningBottom": {
      "type": "object"
    },
    "advanceAnimation": {
      "type": "object",
      "default": {}
    }
  },
  "supports": {
    "className": false
  },
  "keywords": [
    "google",
    "map",
    "direction",
    "path"
  ]
}