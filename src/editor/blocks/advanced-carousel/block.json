{
	"apiVersion": 2,
	"name": "gutenverse/advanced-carousel",
	"title": "Advanced Carousel",
	"description": "Create beautiful image carousel that fit with many website style",
	"category": "gutenverse-element",
	"allowCopyStyle": true,
	"attributes": {
		"elementId": {
			"type": "string"
		},
		"sliderStyle": {
			"type": "string",
			"default": "1"
		},
		"customSliderHeight": {
			"type": "boolean",
			"default": false
		},
		"allowPopup": {
			"type": "boolean",
			"default": false
		},
		"loop": {
			"type": "boolean",
			"default": false
		},
		"autoplay": {
			"type": "boolean",
			"default": false
		},
		"delay": {
			"type": "string",
			"default": 3000
		},
		"sliderHeight": {
			"type": "object"
		},
		"sliderEffect": {
			"type": "string",
			"default": "normal"
		},
		"overflowHideContainer": {
			"type": "boolean",
			"default": true
		},
		"items": {
			"type": "array"
		},
		"sliderNumber": {
			"type": "object",
			"default": {
				"Desktop": 1,
				"Tablet": 1,
				"Mobile": 1
			}
		},
		"centeredSlides": {
			"type": "object"
		},
		"spaceBetween": {
			"type": "object"
		}
	},
	"supports": {
		"className": false
	},
	"keywords": []
}
