      "type": "string"
    },
    "positioningLeft": {
      "type": "object"
    },
    "positioningRight": {
      "type": "object"
    },
    "positioningTop": {
      "type": "object"
    },
    "positioningBottom": {
      "type": "object"
    },
    "advanceAnimation": {
      "type": "object",
      "default": {}
    }
