    "elementId": {
      "type": "string"
    },
    "icon": {
      "type": "string",
      "default": "fab fa-wordpress"
    },
    "iconAlign": {
      "type": "object"
    },
    "iconColorOne": {
      "type": "object"
    },
    "iconSize": {
      "type": "object"
    },
    "iconPadding": {
      "type": "object"
    },
    "iconRotate": {
      "type": "object"
    },
    "iconColorTwo": {
      "type": "object"
    },
    "iconColorHoverOne": {
      "type": "object"
    },
    "iconColorHoverTwo": {
      "type": "object"
    },
    "iconView": {
      "type": "string",
      "default": "stacked"
    },
    "iconShape": {
      "type": "string",
      "default": "rounded"
    },
    "iconBorderWidth": {
      "type": "object"
    },
    "iconBorderRadius": {
      "type": "object"
    },
    "url": {
      "type": "string"
    },
    "linkTarget": {
      "type": "string",
      "source": "attribute",
      "selector": "a",
      "attribute": "target"
    },
    "rel": {
      "type": "string",
      "source": "attribute",
      "selector": "a",
      "attribute": "rel"
