{
  "apiVersion": 2,
  "name": "gutenverse/progress-bar",
  "title": "Progress Bar",
  "description": "Create a progress bar of your achivement or something you want to share.",
  "category": "gutenverse-element",
  "attributes": {
    "elementId": {
      "type": "string"
    },
    "style": {
      "type": "string",
      "default": "default"
    },
    "title": {
      "type": "string",
      "default": "Progress Bar"
    },
    "arrowIcon": {
      "type": "string",
      "default": "fas fa-arrow-right"
    },
    "percentage": {
      "type": "int",
      "default": 75
    },
    "duration": {
      "type": "int",
      "default": 3500
    },
    "colorMode": {
      "type": "string",
      "default": "default"
    },
    "barGradient": {
      "type": "array"
    },
    "trackGradient": {
      "type": "array"
    },
    "barColor": {
      "type": "object"
    },
    "trackColor": {
      "type": "object"
    },
    "trackHeight": {
      "type": "object"
    },
    "barRadius": {
      "type": "object"
    },
    "trackRadius": {
      "type": "object"
    },
    "barPadding": {
      "type": "object"
    },
    "barMargin": {
      "type": "object"
    },
    "titleColor": {
      "type": "object"
    },
    "barBoxShadow": {
      "type": "object"
    },
    "titleTypography": {
      "type": "object"
    },
    "titleTextShadow": {
      "type": "object",
      "default": {}
    },
    "percentColor": {
      "type": "object"
    },
    "percentTypography": {
      "type": "object"
    },
    "percentTextShadow": {
      "type": "object",
      "default": {}
    },
    "background": {
      "type": "object",
      "default": {}
    },
    "backgroundHover": {
      "type": "object",
      "default": {}
    },
    "border": {
      "type": "object",
      "default": {}
    },
    "boxShadow": {
      "type": "object"
    },
    "borderHover": {
      "type": "object",
      "default": {}
    },
    "boxShadowHover": {
      "type": "object"
    },
    "margin": {
      "type": "object"
    },
    "padding": {
      "type": "object"
    },
    "zIndex": {
      "type": "object"
    },
    "animation": {
      "type": "object",
      "default": {}
    },
    "hideDesktop": {
      "type": "boolean"
    },
    "hideTablet": {
      "type": "boolean"
    },
    "hideMobile": {
      "type": "boolean"
    },
    "positioningType": {
      "type": "object"
    },
    "positioningWidth": {
      "type": "object"
    },
    "positioningAlign": {
      "type": "object"
    },
    "positioningLocation": {
      "type": "string"
    },
    "positioningLeft": {
      "type": "object"
    },
    "positioningRight": {
      "type": "object"
    },
    "positioningTop": {
      "type": "object"
    },
    "positioningBottom": {
      "type": "object"
    },
    "percentBgColor": {
      "type": "object"
    },
    "advanceAnimation": {
      "type": "object",
      "default": {}
    }
  },
  "supports": {
    "className": false
  },
  "keywords": ["progress", "bar", "loading"]
}